{
	"version": "0.2.0",
	"configurations": [
		
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"stopOnEntry": false,
			"sourceMaps": true,
			"debugWebWorkerHost": true,
			"autoAttachChildProcesses": true,
			"trace": true,
			"debugWebviews": true,
			"preLaunchTask": "hxml: extension.hxml",
			
			"outFiles": [
				"${workspaceFolder}/*.js"
			]
		}
	]
}